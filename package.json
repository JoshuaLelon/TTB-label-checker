{
  "name": "ttb-label-checker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix --max-warnings=0 .",
    "typecheck": "tsc --noEmit",
    "check": "tsx scripts/check-runner.ts",
    "check:ci": "tsx scripts/check-runner.ts --ci",
    "check:full": "npm run check && npm run build",
    "check:deps": "depcruise app components lib scripts --config .dependency-cruiser.js",
    "check:no-console": "tsx scripts/check-no-console-bypass.ts",
    "check:memo": "tsx scripts/check-unnecessary-memo.ts",
    "check:html-links": "tsx scripts/check-html-links.ts",
    "check:dead-code": "knip",
    "check:duplicates": "jscpd app components lib scripts",
    "check:guards": "tsx scripts/check-design-guards.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,json,mjs,cjs,md}": "biome check --write --no-errors-on-unmatched"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.575.0",
    "next": "16.1.6",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dependency-cruiser": "^17.3.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "eslint-plugin-error-cause": "^1.2.10",
    "eslint-plugin-no-catch-all": "^1.1.0",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-web-api": "^2.13.0",
    "eslint-plugin-react-x": "^2.13.0",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.9.1",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "husky": "^9.1.7",
    "jscpd": "^4.0.8",
    "knip": "^5.85.0",
    "lint-staged": "^16.2.7",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "ultracite": "^7.2.3"
  }
}
